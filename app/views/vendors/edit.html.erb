<section>
  <h3>Edit Profile</h3>
  <ul class="error">
    <% @vendor.errors.each do |field, msg| %>
      <li>
        <%= field %>: <%= msg %>
      </li>
    <% end %>
  </ul>
  <%= form_for @vendor do |f| %>

    <%= f.label :name %>
    <%= f.text_field :name %>

    <%= f.label :username %>
    <%= f.text_field :username %>

    <%= f.label :photo_url %>
    <%= f.text_field :photo_url %>

    <%= f.label :description %>
    <%= f.text_area :description %>

    <%= f.submit action_name %>
  <% end %>
</section>
